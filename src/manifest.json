{
    "name": "Course Offering Vite",
    "version": "0.3.0.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_title": "Vue Popup"
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "background": {
        "service_worker": "background.ts"
    },
    "content_scripts": [
        {
            "matches": ["https://registrar.kfupm.edu.sa/courses-classes/course-offering/"],
            "js": ["embedded/index.ts"],
            "run_at": "document_start"
        }
    ]
}